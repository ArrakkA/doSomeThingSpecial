<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd">

<mapper namespace="gms.something.special.domain.inventory">
    <insert id="realAmount" parameterType="gms.something.special.domain.inventory.dto.realAmountRequestDTO">
        insert into pd_real(
            codiv
            ,pd_month
            ,pd_shop_cd
            ,pd_cd
            ,pd_jego_qty
            ,pd_chul_qty
            ,input_staff
            ,input_datetime
            ,input_ip
            ) values (
            #{codiv}
            ,#{month}
            ,#{shopCd}
            ,#{pdCd}
            ,#{pdJegoQty}
            ,#{pdChulQty}
            ,#{staff}
            ,#{dateTime}
            ,#{ip}
           )
    </insert>
</mapper>