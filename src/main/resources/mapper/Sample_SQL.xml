<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd">

<mapper namespace="gms.something.special.domain.sample.SampleRepository">

    <insert id="save1" parameterType="gms.something.special.domain.sample.SampleDTO">
        insert into aaa( aaa, bbb)
            values (#{aaa}, #{bbb})
    </insert>

    <select id="find1" resultType="String">
        select a.bbb from aaa a
        where a.aaa = #{aaa}
    </select>

</mapper>
